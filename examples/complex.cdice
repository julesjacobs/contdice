let x = uniform(0, 1) in
let y = uniform(0, 1) in
let z = uniform(0, 1) in
if x < 0.5 then
  if y < 0.5 then
    if z < 0.5 then uniform(0, 0) else uniform(1, 1)
  else
    if z < 0.5 then uniform(2, 2) else uniform(3, 3)
else
  if y < 0.5 then
    if z < 0.5 then uniform(4, 4) else uniform(5, 5)
  else
    if z < 0.5 then uniform(6, 6) else uniform(7, 7)